DirectoryIndex Wui.php
Options +FollowSymLinks
RewriteEngine On
 
RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f
 
RewriteRule ^users/(\d+)*$ ./Wui.php?run=$1&argument=void
RewriteRule ^threads/(\d+)*$ ./Wui.php?run=$1&argument=void
 
RewriteRule ^search/(.*)$ ./Wui.php?run=$1&argument=void